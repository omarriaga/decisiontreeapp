        <th>Acciones</th>
        <th>NRO_FOTOS</th>
        <th>REPUBLICADO</th>
        <th>DIAS_PUBLICACION</th>
        <th>TIENE_AREA</th>
        <th>TIENE_BARRIO</th>
        <th>TIENE_CIUDAD</th>
        <th>TIENE_COMENTARIO</th>
        <th>TIENE_NRO_BANOS</th>
        <th>TIENE_NRO_CUARTOS</th>
        <th>TIENE_NRO_GARAJES</th>
        <th>TIENE_VIDEO</th>
      </tr>
      </thead>
      <tbody>
      <% @list.each do |row| %>
